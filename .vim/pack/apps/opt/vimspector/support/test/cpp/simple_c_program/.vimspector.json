{
  "configurations": {
    "CodeLLDB": {
      "adapter": "CodeLLDB",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/test",
        "stopAtEntry": true
      }
    },
    "lldb-vscode": {
      "adapter": "lldb-vscode",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/test",
        "stopAtEntry": true
      }
    },
    "cpptools": {
      "adapter": "vscode-cpptools",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/test",
        "stopOnEntry": true,
        "MIMode": "lldb"
      }
    }
  }
}
