{
  "configurations": {
    "launch current file": {
      "adapter": "cust_vscode-ruby",
      "configuration": {
        "request": "launch",
        "program": "${file}",
        "args": [ "*${args}" ],
        "useBundler": true,
        "trace": true,
        "showDebuggerOutput": true
      }
    }
  }
}
